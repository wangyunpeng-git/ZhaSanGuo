<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/sp"
    tools:context=".activity.SchemaActivity">

    <Button
        android:id="@+id/schema_return_bt"
        android:layout_width="39dp"
        android:layout_height="43dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/return_button_select" />

    <include
        layout="@layout/schema_item"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="10dp"/>

    <include layout="@layout/schema_person_id_item"
        android:layout_height="100dp"
        android:layout_width="150dp"
        android:layout_marginLeft="20dp"/>


</LinearLayout>